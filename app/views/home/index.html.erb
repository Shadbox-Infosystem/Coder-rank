<div class="container">
  <div class="row mt-4">
    <% @data.each do |data|%>
    <div class="col-sm-6">
      <div class="card">
        <div class="card-body">
          <h5 class="card-title"><%= data.test_type %></h5>
          <p class="card-text"><%= data.instruction %></p>
          <%= button_to 'Delete', test_path(data), method: :delete, data: { confirm: 'Are you sure you want to delete this test?' }, class: 'btn btn-danger' %>


        </div>
      </div>
    </div>
    <%end%>
  </div>
</div>
